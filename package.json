{
  "name": "lib-address",
  "description": "A library to format and validate addresses based on Google's open-source address metadata.",
  "homepage": "https://github.com/LancelotP/lib-address",
  "repository": {
    "type": "git",
    "url": "https://github.com/LancelotP/lib-address.git"
  },
  "bugs": {
    "url": "https://github.com/LancelotP/lib-address/issues"
  },
  "author": {
    "email": "lancelot@prigent.dev",
    "name": "Lancelot Prigent"
  },
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.18.2"
  },
  "packageManager": "pnpm@8.10.0",
  "keywords": [
    "address",
    "metadata",
    "country",
    "formatter",
    "validator",
    "iso",
    "iso 3166-1",
    "iso 3166-2"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "scripts:download": "ts-node-esm -P tsconfig.scripts.json scripts/download-metadata.ts"
  },
  "browser": "./dist/entry-browser.mjs",
  "main": "./dist/entry-node.cjs",
  "types": "./dist/entry-node.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@vitest/coverage-v8": "^0.34.6",
    "axios": "^1.6.0",
    "axios-retry": "^3.8.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
  }
}
